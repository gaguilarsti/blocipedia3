<% if current_user.premium? || current_user.admin? %>
  <div class="form-group">
    <%= f.label :private, class: 'checkbox' do %>
      <%= f.check_box :private %> Make this a private wiki?
    <% end %>
  </div>
<% else %>
  Want to make this a private Wiki? <%= link_to "Upgrade Here.", new_charge_path %>
<% end %>
